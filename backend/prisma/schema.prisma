generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model report {
  id    Int     @id @default(autoincrement())
  name  String? @db.VarChar
  cf    BigInt?
  copol Json?   @db.Json
}

model sat {
  id       Int     @id @default(autoincrement())
  name     String? @db.VarChar
  position String? @db.VarChar
}
